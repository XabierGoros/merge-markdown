{
  "name": "merge-markdown",
  "version": "0.1.0",
  "description": "Creates a single markdown file (with TOC) from several markdown files",
  "repository": {
    "type": "git",
    "url": "git@github.com:knennigtri/merge-markdown.git"
  },
  "author": "Kevin Nennig <nen.devpr@gmail.com>",
  "scripts": {
    "default": "merge-markdown -m test/src/manifest.json",
    "short": "merge-markdown -m test/src/manifest-short.json",
    "subFolder": "merge-markdown -m test/src/manifest-subfolder.json",
    "yamlTest": "merge-markdown -m test/src/noyaml/yamlTest",
    "readmeTOC": "doctoc README.md --github --title='# Contents'"
  },
  "main": "index.js",
  "keywords": [
    "markdown",
    "merge",
    "toc",
    "removeYAML",
    "noYAML"
  ],
  "license": "ISC",
  "dependencies": {
    "concat": "^1.0.3",
    "doctoc": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.1.3",
    "markdown-link-check": "^3.8.7",
    "minimist": "^1.2.5",
    "valid-url": "^1.0.9"
  },
  "bin": {
    "merge-markdown": "bin/global.js"
  },
  "preferGlobal": true,
  "directories": {
    "test": "test"
  },
  "devDependencies": {},
  "bugs": {
    "url": "https://github.com/knennigtri/merge-markdown/issues/new/choose"
  }
}
